{
    "models": {
      "object_detection": {
        "path": "models/object_detection_model.pt",
        "architecture": "fasterrcnn_resnet50_fpn",
        "num_classes": 2
      },
      "classification": {
        "path": "models/classification_model.pt",
        "architecture": "resnet50",
        "num_classes": 5
      }
    },
    "classes": {
      "object_detection": {
        "0": "background",
        "1": "leaf"
      },
      "classification": {
        "0": "Healthy",
        "1": "Powdery Mildew",
        "2": "Leaf Rust",
        "3": "Blight",
        "4": "Yellow Spot"
      }
    },
    "tasks": ["object_detection", "classification"],
    "detection_threshold": 0.5,
    "preprocessing": {
      "classification": {
        "resize": [224, 224],
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225]
      }
    }
  }
  